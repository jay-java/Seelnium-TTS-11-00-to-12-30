package core;
//Unary -> ++, --
//Arithmetic -> +,-,*,/,%
//Relational 	-> >,<,>=,<=
//Logical -> && , ||
//Assignment -> =,!=
public class Operators {
	public static void main(String[] args) {
		int i=4;
		i--;  // i = i+1
		System.out.println(i);
		int a = 10,b;
		b = a++;
		System.out.println(b);
	}
}
